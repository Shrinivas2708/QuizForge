{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext"
    ],
    "rootDir": ".",
    "outDir": "./dist",
    "jsx": "react-jsx",
     "types": ["node"],
    "jsxImportSource": "hono/jsx",
     "esModuleInterop": true,
     "allowSyntheticDefaultImports": true
  },
  "include": ["vitest.config.ts"]
}